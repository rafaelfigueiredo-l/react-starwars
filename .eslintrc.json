{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": ["plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "no-console": "error",
    "prettier/prettier": "error",
    "arrow-body-style": ["error", "never"]
  }
}
